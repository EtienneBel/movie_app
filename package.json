{
    "name": "netflix_clone",
    "version": "1.0.0",
    "scripts": {
        "start": "concurrently \"npm run server\" \"npm run client\"",
        "server": "cd server && DEBUG=netflix-server-express:* npm start",
        "client": "cd client && npm run dev",
        "install-all": "npm install && cd server && npm install && cd ../client && npm install"
    },
    "devDependencies": {
        "concurrently": "^6.2.0"
    },
    "dependencies": {
        "dotenv": "^16.4.5"
    }
}
